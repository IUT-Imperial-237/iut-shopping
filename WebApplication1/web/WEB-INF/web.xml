<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <filter>
        <filter-name>AdminFilter</filter-name>
        <filter-class>adminMangement.AdminFilter</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>AdminFilter</filter-name>
        <servlet-name>adminManagement</servlet-name>
    </filter-mapping>
    <servlet>
        <servlet-name>adminManagement</servlet-name>
        <servlet-class>adminMangement.adminManagement</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>adminMangement.login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>userLogin</servlet-name>
        <servlet-class>userLogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>usersLogin</servlet-name>
        <servlet-class>users.usersLogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>usersManagement</servlet-name>
        <servlet-class>users.usersManagement</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>adminManagement</servlet-name>
        <url-pattern>/adminManagement</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>usersLogin</servlet-name>
        <url-pattern>/usersLogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>usersManagement</servlet-name>
        <url-pattern>/usersManagement</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
